        </TextBlock>

        <ListBox x:Name="ListBoxTest" IsEnabled="{Binding IsChecked, ElementName=EnableListBox}" Margin="13,9,159,10" SelectionChanged="ListBoxTest_SelectionChanged" Grid.Row="1" Grid.Column="1"/>

